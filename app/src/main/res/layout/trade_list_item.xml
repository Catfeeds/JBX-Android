<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <LinearLayout
        android:id="@+id/header_cell"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/sixteen_dp"
        android:layout_marginLeft="@dimen/sixteen_dp"
        android:orientation="horizontal"
        android:weightSum="4">
        <TextView
            android:id="@+id/first_txt"
            android:visibility="visible"
            android:layout_weight="1.5"
            android:layout_width="@dimen/zero_dp"
            android:layout_height="wrap_content"
            android:text="商家名称"
            android:textAlignment="center"
            android:textColor="#FF666666"
            android:textSize="@dimen/threeteen_sp"
            android:paddingBottom="@dimen/five_dpp"
            android:paddingTop="@dimen/five_dpp"/>
        <TextView
            android:id="@+id/second_txt"
            android:visibility="visible"
            android:layout_weight="1.5"
            android:layout_width="@dimen/zero_dp"
            android:layout_height="wrap_content"
            android:text="交易笔数"
            android:paddingBottom="@dimen/five_dpp"
            android:paddingTop="@dimen/five_dpp"
            android:textAlignment="center"
            android:textColor="#FF666666"
            android:textSize="@dimen/threeteen_sp"
            />
        <TextView
            android:id="@+id/three_txt"
            android:visibility="visible"
            android:layout_width="@dimen/zero_dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="总面积(m²)"
            android:paddingBottom="@dimen/five_dpp"
            android:paddingTop="@dimen/five_dpp"
            android:textColor="#FF666666"
            android:textAlignment="center"
            android:textSize="@dimen/threeteen_sp"
            />
        <TextView
            android:id="@+id/four_txt"
            android:visibility="visible"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textColor="#FF666666"
            android:paddingBottom="@dimen/five_dpp"
            android:paddingTop="@dimen/five_dpp"
            android:textSize="@dimen/threeteen_sp"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/sixteen_dp"
        android:layout_marginRight="@dimen/sixteen_dp"
        android:orientation="horizontal"
        android:weightSum="4">
        <LinearLayout
            android:layout_weight="1.5"
            android:layout_width="@dimen/zero_dp"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/r_first_txt"
                android:paddingTop="@dimen/five_dpp"
                android:paddingBottom="@dimen/five_dpp"
                android:background="#F1F1F1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="天津鑫利恒发…"
                android:textAlignment="center"
                android:maxLines="1"
                android:textColor="#FF000000"
                android:textSize="@dimen/twenty_sp"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_weight="1.5"
            android:layout_width="@dimen/zero_dp"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/r_second_txt"
                android:paddingTop="@dimen/five_dpp"
                android:paddingBottom="@dimen/five_dpp"
                android:background="#F1F1F1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="1000"
                android:textAlignment="center"
                android:maxLines="1"
                android:textColor="#FF000000"
                android:textSize="@dimen/twenty_sp"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="@dimen/zero_dp"
            android:visibility="gone"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/r_three_txt"
                android:paddingTop="@dimen/five_dpp"
                android:paddingBottom="@dimen/five_dpp"
                android:background="#F1F1F1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="1000"
                android:textAlignment="center"
                android:maxLines="1"
                android:textColor="#FF000000"
                android:textSize="@dimen/twenty_sp"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_weight="1"
            android:layout_width="@dimen/zero_dp"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/r_four_txt"
                android:paddingTop="@dimen/five_dpp"
                android:paddingBottom="@dimen/five_dpp"
                android:background="#F1F1F1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="刚刚"
                android:textAlignment="center"
                android:maxLines="1"
                android:textColor="#FF000000"
                android:textSize="@dimen/twenty_sp"
                />
        </LinearLayout>


    </LinearLayout>
</LinearLayout>